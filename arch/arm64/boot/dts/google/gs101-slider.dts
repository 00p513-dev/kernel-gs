/dts-v1/;
/plugin/;

/ {
	board_id = <0x201>;
	board_rev = <0x100>;
	fragment@boardbase {
		target-path = "/";
		__overlay__ {
			model = "GS101 Slider board based on GS101";
		};
	};
};

&serial_0 {
	status = "okay";
};

&pinctrl_1 {
	key_voldown: key-voldown {
		samsung,pins = "gpa7-3";
		samsung,pin-function = <0xf>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	key_volup: key-volup {
		samsung,pins = "gpa8-1";
		samsung,pin-function = <0xf>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};
};

&pinctrl_0 {
	key_power: key-power {
		samsung,pins = "gpa10-1";
		samsung,pin-function = <0xf>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};
};
