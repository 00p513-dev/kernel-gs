					channel_enabled;
				};

				ch8: channel@7 {
					rail-name = "BUCK1M";
					channel_enabled;
				};
			};

			rails {
				use-regulators-as-rails;

				/* Additional rails */
				m_ext1_rail: VSEN_V1 {
					external_rail;
					external-chip-en-index = <1>;
					channel-mux-selection = <0x1C>;
					shunt-res-uohms = <10000>;
