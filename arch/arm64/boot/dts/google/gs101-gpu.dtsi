/ {
	mali_mgm: physical-memory-group-manager {
		compatible = "arm,physical-memory-group-manager";
		pt_id = "GPU";
	};

	mali: mali@1C500000 {
		compatible = "arm,malit6xx";
		reg = <0x0 0x1C500000 0x5000>;
		interrupts =
			<GIC_SPI IRQ_G3D_IRQJOB_G3D IRQ_TYPE_LEVEL_HIGH>,
			<GIC_SPI IRQ_G3D_IRQMMU_G3D IRQ_TYPE_LEVEL_HIGH>,
			<GIC_SPI IRQ_G3D_IRQGPU_G3D IRQ_TYPE_LEVEL_HIGH>;
		interrupt-names = "JOB", "MMU", "GPU";

		/* Power */
		power-domains = <&pd_embedded_g3d>;
		g3d_genpd_name = "pd-embedded_g3d";
		gpu_pmu_status_reg_offset = <0x2004>;
		gpu_pmu_status_local_pwr_mask = <0x1>; /*0x1 << 0*/
		gpu_pm_autosuspend_delay = <50>;

		/* Memory */
		physical-memory-group-manager = <&mali_mgm>;

		/* Clocks */
		gpu0_cmu_cal_id = <ACPM_DVFS_G3DL2>;
		gpu1_cmu_cal_id = <ACPM_DVFS_G3D>;

		/* DVFS */
		gpu_dvfs_governor = "basic";
		gpu_dvfs_clockdown_hysteresis = <50>;
		gpu_dvfs_table_size = <6 10>; /*<row col>*/
		gpu_dvfs_table_v1 = <
			/*
			 *gpu0    gpu1   down     up    hys      int      mif   little  middle   big
			 * clk     clk   util   util  ticks      min      min      min     min   max
			 *--------------------------------------------------------------------------
			 */
			996000  670000     78    100      1   465000        0        0       0     0
			885000  572000     78     98      2   465000        0        0       0     0
			750000  455000     78     95      3   332000        0        0       0     0
			434000  302000     78     85      6   200000        0        0       0     0
			302000  151000     50     80      2   200000        0        0       0     0
			151000  151000      0     60      1        0        0        0       0     0
		>;
		gpu_dvfs_table_v2 = <
			/*
			 *gpu0    gpu1   down     up    hys      int      mif   little  middle   big
			 * clk     clk   util   util  ticks      min      min      min     min   max
			 *--------------------------------------------------------------------------
			 */
			996000  701000     78    100      1   465000        0        0       0     0
			885000  572000     78     98      2   465000        0        0       0     0
			750000  471000     78     95      3   332000        0        0       0     0
			603000  351000     78     85      6   200000        0        0       0     0
			302000  202000     50     80      2   200000        0        0       0     0
			151000  151000      0     60      1        0        0        0       0     0
		>;
		/* QOS */
		gpu_dvfs_qos_bts_scenario = "g3d_performance";
		gpu_dvfs_qos_bts_level = <3>;

		/* Thermal */
		#cooling-cells = <2>;
		gpu_power_coeff = <625>;
		ect-coeff-index = <3>;

		/*
		 * Pixel Platform integration log level
		 * (0: off, 1: debug, 2: info, 3: warn, 4: error)
		 */
		gpu_log_level = <2>;

	};

};
