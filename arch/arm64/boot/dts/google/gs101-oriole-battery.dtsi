&google_battery {
	google,chg-battery-capacity = <4520>;
};

&max77759_fg {
	maxim,fg-data {
		#include "gs101-oriole-battery-data.dtsi"
	};
};

/ {
	fragment@battery {
		target-path = "/";
		__overlay__ {
			google_battery: google,battery {
				google,ttf-temp-idx= <2>;
				google,ttf-adapter = <3000>;
				google,ttf-soc-table =  <48 62 68 81 87 91  96 100>;
				google,ttf-elap-table = <38 48 55 68 76 101 158 311>;
				google,ttf-tier-table = <0 64 82>;
			};

			google_bms {
				nvmem-names = "4-00500";
				/* pack eeprom is available only with P20+ batteries */
				google,bee-name = "4-00500";
			};
		};
	};
};
