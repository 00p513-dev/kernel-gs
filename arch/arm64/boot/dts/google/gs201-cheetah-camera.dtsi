					 1000
					 1000>;

				/* Power down sequence */
				power-down-seqs =
					"gpio2",
					"gpio1",
					"S2MPG13_LDO12";
				power-down-seq-types =
					"regulator",
					"regulator",
					"regulator";
				power-down-seq-delays-us =
					<0
					 0
					 0>;
			};

			/* Ois sem1215sa for gm5 */
			ois1: ois@1 {
				compatible = "google,lwis-i2c-device";

				/* Device node name */
				node-name = "ois-sem1215sa";

				/* I2C */
				i2c-bus = <&hsi2c_4>;
				i2c-addr = <0x34>;

				/* Registers */
				reg-addr-bitwidth = <16>;
				reg-value-bitwidth = <8>;

				/* Power sequence */
				regulators =
					<&slg51002_ldo5 &s_ldo12_reg
					&slg51002_gpio1>;
				regulator-voltages =
					<2850000 1800000>;

				/* Power up sequence */
				power-up-seqs =
					"S2MPG13_LDO12",
					"ldo5",
					"gpio1";
				power-up-seq-types =
					"regulator",
					"regulator",
					"regulator";
				power-up-seq-delays-us =
					<1000
					 0
					 0>;

				/* Power down sequence */
				power-down-seqs =
