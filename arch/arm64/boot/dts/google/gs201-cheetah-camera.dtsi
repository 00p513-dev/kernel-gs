					<&slg51002_ldo4 &s_ldo12_reg>;
				regulator-voltages =
					<2900000 1800000>;

				/* Power up sequence */
				power-up-seqs =
					"S2MPG13_LDO12",
					"ldo4";
				power-up-seq-types =
					"regulator",
					"regulator";
				power-up-seq-delays-us =
					<0
					 0>;

				/* Power down sequence */
				power-down-seqs =
					"ldo4",
					"S2MPG13_LDO12";
				power-down-seq-types =
					"regulator",
					"regulator";
				power-down-seq-delays-us =
					<0
					 0>;
			};

			/* Ois lc898128 for gn1*/
			ois0: ois@0 {
				compatible = "google,lwis-i2c-device";

				/* Device node name */
				node-name = "ois-lc898128";

				/* I2C */
				i2c-bus = <&hsi2c_1>;
				i2c-addr = <0x24>;

				/* Registers */
				reg-addr-bitwidth = <16>;
				reg-value-bitwidth = <32>;

				/* Power sequence */
				regulators =
					<&s_ldo12_reg &slg51002_gpio2
					&slg51002_gpio1>;
				regulator-voltages =
