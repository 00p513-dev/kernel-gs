#include "gs101-raviole-battery.dtsi"
#include "gs101-raviole-usecases.dtsi"

&google_battery {
	google,chg-battery-capacity = <5003>;
};

&max77759_fg {
	maxim,fg-data {
		#include "gs101-raven-battery-data.dtsi"
	};
};

&pca9468_dc {
	pca9468,charging-current = <5000000>; /* 5A */
	pca9468,input-current-limit = <3000000>; /* 3.0A */
};

/ {
	fragment@battery {
		target-path = "/";
		__overlay__ {
			google_bms {
				nvmem-names = "5-00500";
				/* pack eeprom is available only with P20+ batteries */
				google,bee-name = "5-00500";
			};
		};
	};
};